{
  "name": "knowme.api",
  "version": "0.0.1",
  "description": "api built to get my portfolio's information",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sarthakjdev/knowme.API"
  },
  "scripts": {
    "build": "tsc",
    "start": " npm run build && node -r ./prod-paths.ts ./dist/index.js NODE_ENV=production",
    "dev:db": "docker-compose -f ./docker/docker-compose.yaml down && docker-compose -f ./docker/docker-compose.yaml up -d ",
    "dev": "ts-node-dev -r  tsconfig-paths/register --respawn --exit-child --ignore-watch swagger_output.json --max-old-space-size=4076 ./index.ts",
    "dev:db-server": "npm run dev:db && npm run dev",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "db-migrate": "npx prisma generate && npx prisma db push",
    "db-generate": "npx prisma generate",
    "prepare": "husky install",
    "swagger-docs": "npm run build && node ./dist/src/utils/swagger.js"
  },
  "keywords": [],
  "author": {
    "name": "Sarthak Jain",
    "email": "contact.sarthakjain@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@dytesdk/web-core": "^0.40.0",
    "@google-cloud/local-auth": "^2.1.0",
    "@prisma/client": "^4.3.1",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "googleapis": "^105.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.4",
    "swagger-autogen": "^2.22.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "tsconfig-paths": "^3.14.1",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "prisma": "^4.3.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
