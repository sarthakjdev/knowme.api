{
  "name": "knowme.api",
  "version": "0.0.1",
  "description": "api built to get my portfolio's information",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sarthakjdev/knowme.API"
  },
  "scripts": {
    "build": "tsc",
    "start": " npm run build && node -r ./prod-paths.ts ./dist/index.js NODE_ENV=production",
    "dev:db": "docker-compose -f ./docker/docker-compose.yaml down && docker-compose -f ./docker/docker-compose.yaml up -d ",
    "dev": "ts-node-dev -r  tsconfig-paths/register --respawn --max-old-space-size=4076 ./index.ts",
    "dev:db-server": "npm run dev:db && npm run dev",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "db-migrate": "npx prisma generate && npx prisma db push"
  },
  "keywords": [],
  "author": {
    "name": "Sarthak Jain",
    "email": "contact.sarthakjain@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "tsconfig-paths": "^3.14.1",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^17.0.23",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-plugin-import": "^2.26.0",
    "prisma": "^4.3.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
